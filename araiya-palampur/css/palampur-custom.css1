@media only screen and (max-width: 767px) {
  .activity-page .carousel-wrap{
    margin: 0px 20px;
  }
  .desc-activity-sec{
    margin-bottom: 20px;
  }
  .owl-nav .owl-prev {
    left: -30px;
    position: absolute;
    max-width: 100px;
    height: auto;
  }
  
  .owl-nav .owl-next {
    right: -30px;
    position: absolute;
    max-width: 100px;
    height: auto;
  }
  .summer-offer .owl-nav .owl-next {
      right: -10px;
      position: absolute;
      max-width: 100px;
      height: auto;
  }
  .summer-offer   .owl-nav .owl-prev {
      left: -10px;
      position: absolute;
      max-width: 100px;
      height: auto;
  }
  .fullscreen-carousel.carousel{
    margin-top: 50px;
  }
  .caption-box h4{
    font-size: 18px !important;
  }
  .caption-box p{
    font-size: 16px
  }
}
@media screen and (max-width: 640px){
  .fullscreen-carousel .carousel-item{
    background: no-repeat left top cover;
    background-position: left;
  }
}
@media only screen and (max-width: 400px) {
  .caption-box h4{
    font-size: 16px !important;
  }
}